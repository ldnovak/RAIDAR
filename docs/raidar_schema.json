{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/product.schema.json",
    "title": "Asset",
    "description": "An open source music resource",
    "type": "object",
    "properties": {
        "AssetHeaderInfo": {
            "ID": {
                "description": "The title of the album",
                "type": "string"
            },
            "LEADER": {
                "description": "The title of the album",
                "type": "string"
            },
            "Date": {
                "description": "The title of the album",
                "type": "string"
            },
            "ControlID": {
                "description": "The title of the album",
                "type": "string"
            },
            "Receiver": {
                "description": "The title of the album",
                "type": "string"
            },
            "Sender": {
                "description": "The title of the album",
                "type": "string"
            },
            "Token": {
                "description": "The title of the album",
                "type": "string"
            },
            "Title": {
                "description": "The title of the album",
                "type": "string"
            },
            "SubTitle": {
                "description": "The title of the album",
                "type": "string"
            },
            "Copyright": {
                "description": "The title of the album",
                "type": "string"
            }
        },
        "AssetResourceList": {
            "description": "The title of the album",
            "type": "array",
            "AudioRecording": {
                "description": "The music track (vocal or instrument)",
                "Date": {
                    "description": "The title of the album",
                    "type": "string"
                },
                "Duration": {
                    "description": "The title of the album",
                    "type": "string"
                },
                "Title": {
                    "description": "The title of the album",
                    "type": "string"
                },
                "ID": {
                    "description": "The title of the album",
                    "type": "object",
                    "propertiies": {
                        "ISRC": {
                            "description": "The ISRC",
                            "type": "string"
                        },
                        "required": []
                    }
                },
                "Type": {
                    "description": "The title of the album",
                    "type": "string"
                },
                "Reference": {
                    "description": "The title of the album",
                    "type": "string"
                },
                "ReferenceTitle": {
                    "description": "The Sound Recording reference Title",
                    "type": "object",
                    "propertiies": {
                        "TitleText": {
                            "description": "The Title Text",
                            "type": "string"
                        },
                        "required": []
                    }
                },
                "display_artist": {
                    "type": "object",
                    "properties": {
                        "full_names": {
                            "type": "array",
                            "full_name": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "description": "The id of the artist",
                                        "type": "integer"
                                    },
                                    "name": {
                                        "description": "The name of the artist",
                                        "type": "string"
                                    },
                                    "id_space": {
                                        "description": "The identiy origin of the artist",
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "id",
                                    "name",
                                    "id_space"
                                ]
                            }
                        },
                        "ContactInfo": {
                            "description": "The identiy origin of the artist",
                            "type": "object",
                            "properties": {
                                "address": {
                                    "description": "The address of the artist",
                                    "type": "string"
                                },
                                "email": {
                                    "description": "The email address of the artist",
                                    "type": "string"
                                },
                                "phone": {
                                    "description": "The email address of the artist",
                                    "type": "string"
                                },
                                "officialWebsite": {
                                    "description": "The email address of the artist",
                                    "type": "string"
                                }
                            }
                        },
                        "Role": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        },
                        "GroupName": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        },
                        "Reference": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        },
                        "LabelName": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        },
                        "RecordingRight": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        },
                        "Lyrics": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        },
                        "Genre": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        },
                        "AudioTechnicalDetail": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        },
                        "Ethnicity": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        },
                        "Gender": {
                            "description": "The date of the performance in UTC",
                            "type": "string"
                        }
                    },
                    "required": [
                        "full_names",
                        "address",
                        "date",
                        "email"
                    ]
                },
                "minItems": 1,
                "uniqueItems": true,
                "date": "utc"
            },
            "VideoRecording": {},
            "Movie": {}
        },
        "Concert": {}
    }
}